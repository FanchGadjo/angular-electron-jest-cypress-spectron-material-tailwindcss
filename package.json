{
  "name": "angular-electron-jest-cypress-spectron-material-tailwindcss",
  "version": "0.0.0",
  "description": "Angular application in Electron",
  "homepage": "angular-electron-jest-cypress-spectron-material-tailwindcss",
  "author": {
    "email": "gael.clair@gmail.com",
    "name": "GaÃ«l Clair"
  },
  "main": "electron/dist/main.js",
  "private": true,
  "scripts": {
    "commit": "cz",
    "ng": "ng",
    "start": "npm-run-all -l -p ng:tw:start electron:start",
    "build": "npm-run-all -l -p ng:tw:build:prod electron:build:prod && electron-builder build",
    "build:ci": "npm-run-all -l -p ng:tw:build:prod electron:build:prod",
    "builder": "electron-builder build --win",
    "e2e": "npm-run-all -l -p ng:build:prod electron:build e2e:start",
    "e2e:start": "npm-run-all -l e2e:wait e2e:run",
    "e2e:ci": "npm-run-all -l e2e:wait \"e2e:run -- --ci --runInBand\"",
    "e2e:wait": "wait-on src/dist/index.html && wait-on electron/dist/main.js",
    "e2e:run": "jest --config e2e/jest.config.js",
    "test": "npm-run-all -l ng:test electron:test",
    "test:watch": "npm-run-all -l -p ng:test:watch electron:test:watch",
    "test:ci": "npm-run-all -l -p ng:test:ci electron:test:ci",
    "lint": "ng lint",
    "lint:fix": "npm run lint -- --fix",
    "mkdir:reports": "make-dir reports",
    "lint:ci": "npm run mkdir:reports && ng lint --format junit > reports/lint.xml",
    "lint:style": "stylelint \"src/app/**/*.scss\" \"src/styles.scss\" \"src/tailwind.css\"",
    "lint:style:ci": "npm run lint:style -- --custom-formatter stylelint-junit-file-formatter.js",
    "lint:style:fix": "npm run lint:style -- --fix",
    "format": "prettier --write .",
    "ng:doc": "compodoc -p src/tsconfig.app.json --theme material",
    "ng:clean": "rimraf src/dist",
    "preng:build": "npm run ng:clean",
    "ng:build": "ng build --base-href ./",
    "ng:build:watch": "npm run ng:build -- --watch",
    "ng:build:prod": "npm run ng:build -- --prod",
    "ng:tw:build": "npm run tw:build && npm run ng:build",
    "ng:tw:build:watch": "npm run tw:build && npm-run-all -l -p ng:build:watch tw:watch",
    "ng:tw:build:prod": "npm run tw:build -- --purge && npm run ng:build -- --prod",
    "ng:tw:start": "npm run tw:build && npm-run-all -l -p ng:serve tw:watch",
    "ng:serve": "ng serve",
    "ng:e2e": "npm-run-all -l -p -r ng:e2e:start ng:cypress:open",
    "ng:e2e:ci": "npm-run-all -l -p -r ng:e2e:start ng:cypress:ci",
    "ng:e2e:start": "npm run tw:build && npm-run-all -l -p ng:e2e:serve tw:watch",
    "ng:e2e:serve": "ng e2e",
    "ng:cypress:open": "wait-on http://localhost:4200 && cypress open",
    "ng:cypress:ci": "wait-on http://localhost:4200 && cypress run --reporter junit --reporter-options mochaFile=reports/angular/ti.xml",
    "ng:test": "jest --config src/jest.config.js",
    "ng:test:watch": "npm run ng:test -- --watch",
    "ng:test:cov": "npm run ng:test -- --coverage",
    "ng:test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --config src/jest.config.js",
    "ng:test:ci": "npm run ng:test:cov -- --ci --runInBand",
    "tw:build": "ngtw build",
    "tw:watch": "ngtw watch",
    "electron:ng:start": "npm-run-all -l -p ng:tw:start electron:ng:serve",
    "electron:ng:start:build": "npm-run-all -l ng:tw:build electron:build electron:serve:build",
    "electron:ng:serve": "wait-on http://localhost:4200 && npm run electron:start",
    "electron:start": "npm-run-all -l -p electron:build:watch electron:serve",
    "electron:clean": "rimraf electron/dist",
    "preelectron:build": "npm run electron:clean",
    "electron:build": "tsc -p electron/tsconfig.json",
    "electron:build:prod": "npm run electron:build -- --sourceMap false",
    "electron:build:watch": "npm run electron:build -- --watch",
    "electron:serve": "wait-on electron/dist/main.js && npm run electron:open -- --serve",
    "electron:serve:build": "wait-on src/dist/index.html && wait-on electron/dist/main.js && npm run electron:open",
    "electron:open": "electron .",
    "electron:test": "jest --config electron/jest.config.js",
    "electron:test:watch": "npm run electron:test -- --watch",
    "electron:test:cov": "npm run electron:test -- --coverage",
    "electron:test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --config electron/jest.config.js",
    "electron:test:ci": "npm run electron:test:cov -- --ci --runInBand"
  },
  "dependencies": {
    "@angular/animations": "~10.1.4",
    "@angular/cdk": "^10.2.4",
    "@angular/common": "~10.1.4",
    "@angular/compiler": "~10.1.4",
    "@angular/core": "~10.1.4",
    "@angular/forms": "~10.1.4",
    "@angular/material": "^10.2.4",
    "@angular/platform-browser": "~10.1.4",
    "@angular/platform-browser-dynamic": "~10.1.4",
    "@angular/router": "~10.1.4",
    "commitizen": "^4.2.2",
    "electron-log": "^4.2.4",
    "hammerjs": "^2.0.8",
    "leveldown": "^5.6.0",
    "momentjs": "^2.0.0",
    "nanoid": "^3.1.15",
    "pouchdb-adapter-indexeddb": "^7.2.2",
    "pouchdb-adapter-leveldb": "^7.2.2",
    "rxdb": "^9.6.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.4",
    "@angular/cli": "~10.1.4",
    "@angular/compiler-cli": "~10.1.4",
    "@briebug/cypress-schematic": "^4.2.0",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@compodoc/compodoc": "^1.1.11",
    "@cypress/code-coverage": "^3.8.1",
    "@cypress/webpack-preprocessor": "5.4.6",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/jest": "^26.0.14",
    "@types/leveldown": "^4.0.2",
    "@types/moment": "^2.13.0",
    "@types/node": "^14.11.5",
    "codelyzer": "^6.0.0",
    "cypress": "5.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "electron": "^10.1.3",
    "electron-builder": "^22.9.1",
    "electron-reload": "^1.5.0",
    "husky": "^4.3.0",
    "istanbul-instrumenter-loader": "^3.0.1",
    "istanbul-lib-coverage": "^3.0.0",
    "jest": "^26.5.2",
    "jest-html-reporter": "^3.2.0",
    "jest-junit": "^12.0.0",
    "jest-preset-angular": "^8.3.1",
    "lint-staged": "^10.4.0",
    "make-dir": "^3.1.0",
    "make-dir-cli": "^2.0.0",
    "ng-tailwindcss": "^2.3.1",
    "ngx-build-plus": "^10.1.1",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.19",
    "spectron": "^12.0.0",
    "stylelint": "^13.7.2",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-junit-formatter": "^0.2.2",
    "stylelint-no-unsupported-browser-features": "^4.0.0",
    "stylelint-prettier": "^1.1.2",
    "tailwindcss": "^1.8.11",
    "ts-jest": "^26.4.1",
    "ts-loader": "8.0.4",
    "ts-node": "^9.0.0",
    "tslint": "~6.1.0",
    "tslint-angular": "^3.0.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "~4.0.2",
    "wait-on": "^5.2.0"
  }
}
